    /* Custom styles for the search box */
    .form-outline {
      position: relative;
      width: 300px; /* Set the width of the search box */
      margin: 0 auto; /* Center the search box */
    }
  
    .form-outline input[type="search"] {
      width: 100%;
      padding: 10px;
      font-size: 16px;
      border: 2px solid #030200;
      border-radius: 5px;
    }
  
    .form-outline label {
      position: absolute;
      top: 10px;
      left: 10px;
      background-color: white;
      padding: 0 5px;
      font-size: 14px;
      color: #ff9100;
    }
  
    .form-outline input[type="search"]:focus + label,
    .form-outline input[type="search"]:not(:placeholder-shown) + label {
      transform: translate(-5px, -25px);
      background-color: white;
      padding: 0 5px;
      font-size: 12px;
      color: #080400;
    }
  
    .form-outline button {
      background-color: #ffa200;
      color: white;
      border: none;
      border-radius: 5px;
      padding: 10px 20px;
      margin-left: -50px; /* Adjust the margin to align with the search box */
    }



    body {
