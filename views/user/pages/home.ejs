  margin-top: -70px;

}

.logo-container {
  display: inline-block; 
}

.logo {
  max-width: 600px;
  max-height: 600px; /* Adjust the size as needed */
  
}

.section-1-div-1-text-1 {
  font-size: 2.8rem;
  line-height: 1.6;
  font-weight: 450;
  color: rgba(254, 250, 250, 0.9);
}


.button-section-1 {
  display: grid;
  grid-template-columns: repeat(2, 1fr); /* 2 columns */
  gap: 15px; /* Adjusted gap between buttons */
  position: absolute;
  bottom: 40px;
  width: 100%;
  max-width: 600px;
