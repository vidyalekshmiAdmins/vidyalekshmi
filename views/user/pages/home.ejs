}

.u-btn-1 {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10px 20px; /* Reduced height for the buttons */
  font-size: 0.875rem;
  font-weight: bold;
  text-transform: uppercase;
  color: white;
  text-decoration: none;
  border: 1px solid white;
  border-radius: 30px;
  transition: background 0.3s, color 0.3s;
  box-sizing: border-box;
}

.u-btn-1:hover {
  background: rgba(255, 255, 255, 0.2);
  color: #85d4f1;
}

/* Media query for responsiveness (optional) */
@media (max-width: 600px) {
  .button-section-1 {
    max-width: 100%;
    right: 0;
    bottom: 20px;
  }
}


.section-2 {
  background-color: #f4f6f3;
  text-align: center;
  min-height: 100vh;
}

.header-section {
  background-color: #f4f6f3; /* Soft green background */
  border-top: 50px solid #8fa98d; /* Slightly darker green border */
  border-bottom: 5px solid #8fa98d; 
  width: 100vw;
  text-align: center; /* Center align the text */
  padding: 20px 0; /* Vertical padding for spacing */
  margin: 0; /* No margin for alignment */
}

.header-section h1 {
  font-family: 'Arial', sans-serif; /* Clean sans-serif font */
  font-weight: 600; /* Medium font weight */
  color: #4F5E4D; /* Dark green text */
  font-size: 27px; /* Adjust font size */
  margin: 0; /* Remove default margins */
}



.carousel {
  padding-top: 60px
  ;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}

.carousel-track-container {
  overflow: hidden;
  width: 80%; /* Adjust width to fit three slides */
}

.carousel-track {
  display: flex;
  transition: transform 0.5s ease-in-out;
  list-style: none;
  padding: 0;
  margin: 0;
}

.carousel-slide {
  flex: 0 0 calc(33.33% - 20px); /* 33.33% width for three slides */
  margin: 0 10px;
  border-radius: 55px; /* Apply rounded corners */
  overflow: hidden; /* Ensures content stays within the rounded corners */
}

.card {
  position: relative;
  background-color: #fff;
  padding: 20px;
  box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
  border: 2px;
  border-radius: 25px; /* Apply a uniform rounded corner */
  text-align: center;
  transform: scale(0.9);
  transition: transform 0.3s ease;
}


.card:hover {
  transform: scale(1);
  background-color: #000000;
}

.card img {
  width: 100%;
  border-radius: 8px;
  margin-bottom: 15px;
}

.card h3 {
  font-size: 1.6rem;
  margin: 10px 0;
  color: #333;
}

.card p {
  font-size: 1.3rem;
  color: #666;
}

.carousel-arrow {
  position: absolute; 
  top: 50%;
  transform: translateY(-50%);
  cursor: pointer;  /* Keep the cursor style */
  z-index: 1000;
  background-color: transparent;  /* Remove background */
  border: none;  /* Remove any border */
  padding: 0;  /* Remove padding */
  width: 20px;  /* Define width if necessary for the arrow icon */
  height: 20px;  /* Define height if necessary for the arrow icon */
}

.carousel-arrow:hover {
  background-color: #000305;
}

.left-arrow {
  left: 10px;
}

.right-arrow {
  right: 10px;
}





.section-3 {
  position: relative;
  width: 100%;
  height: 130vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  min-height: 1094px;
}




.section-3::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 30%;
  background-color: #84d9f7;
}


.section-3::after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 70%;
  background-color: #0d244b;
}


.section-3 .grid-container {
  display: flex;
  flex-wrap: wrap;
  gap: 10px;
  max-height: 455px;
  width: 100%;
  z-index: 1;
  padding: 30px;
}


.section-3 .grid-item {
  flex: 1;
margin-top: 100px;
  width: fit-content;


}

.section-3 .grid-item img {
  max-width: 90%; 
  max-height: 90%;
  object-fit: cover;
  box-shadow: 5px 5px 20px 0 rgba(0,0,0,0.4);
  background-position: 50% 50%;
  border-radius: 5px;
}



.section-3 .grid-container-2 {
  display: flex;
  flex-wrap: wrap;
  gap: 10px;
  max-height: 300px;
  width: 100%;
  z-index: 1;
}

/* Feature list */
.section-3 .feature-list {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  grid-gap: 20px;
  height: 440px;
  width: 100%;
  margin-top: 100px;

  z-index: 1;
  padding: 10px;
}

/* Feature item */
.section-3 .feature-item {
  text-align: center;
  padding: 2px;
  border-radius: 20px;
  background-color: white;
  width: 90%;
  
}

.section-3 .feature-item img{
  display: block;
  margin: 30px;
 width: 70px; 
  height: 70px;

}


.section-3 .feature-item .icon {
  margin: 30px;
  margin-left: 100px;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  background-color: #f1c50e;
  box-shadow: 4px -4px 10px rgba(0, 0, 0, 0.377);
  width: 100px;
  height: 100px;
}





.feature-item .icon i {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 1; /* Ensure icon is above the circle background */
}

/* Adjust icon color */
.feature-item .icon i {
  color: black; /* Change icon color as needed */
}
</style>


